&& (request.resource.data.userId == request.auth.uid ||\n                                          resource.data.userId == request.auth.uid));\n      allow update, delete: if isAdmin() || (signedIn() && resource.data.authorId == request.auth.uid);\n    }\n\n    match /forumPosts/{postId} {\n      allow read: if true;\n      allow create: if signedIn();\n      allow update, delete: if isAdmin() || (signedIn() && resource.data.userId == request.auth.uid);\n      match /replies/{replyId} {\n        allow read: if true;\n        allow create: if signedIn();\n        allow update, delete: if isAdmin() || (signedIn() && resource.data.userId == request.auth.uid);\n      }\n    }\n\n    match /learningModules/{moduleId} {\n      allow read: if true;\n      allow write: if isAdmin() || isStaff();\n    }\n\n    match /adminLogs/{logId} {\n      allow read, create: if isAdmin() || isStaff();\n      allow update, delete: if isAdmin();\n    }\n\n    match /{document=**} { allow read, write: if false; }\n  }\n}\n\n"}]}}
