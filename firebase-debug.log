[debug] [2025-08-05T22:14:16.460Z] >>> [apiv2][body] PUT http://127.0.0.1:8080/emulator/v1/projects/demo-app:securityRules {"ignore_errors":true,"rules":{"files":[{"name":"security.rules","content":"\nrules_version = '2';\n\nservice cloud.firestore {\n  match /databases/{database}/documents {\n\n    // Helper functions to make rules cleaner\n    function isSignedIn() {\n      return request.auth != null;\n    }\n\n    function isOwner(userId) {\n      return request.auth.uid == userId;\n    }\n\n    // Check for admin custom claim\n    function isAdmin() {\n      return request.auth.token.admin == true;\n    }\n\n    // USER DATA\n    // Users can read and update their own profile.\n    // Admins can read and update any user profile.\n    match /users/{userId} {\n      allow read, update: if isOwner(userId) || isAdmin();\n      allow create: if isSignedIn();\n    }\n\n    // USER SUBCOLLECTIONS\n    match /users/{userId}/{subcollection}/{docId} {\n      // Users can manage their own application drafts, notifications, messages, etc.\n      allow read, write, create, delete: if isOwner(userId);\n    }\n    \n    // ADMIN-ONLY LISTS\n    // Admins need list access for the main admin tables.\n    match /applications/{applicationId} {\n      allow list: if isAdmin();\n      allow read: if isAdmin() || get(/databases/$(database)/documents/applications/$(applicationId)).data.userId == request.auth.uid;\n      allow create: if isSignedIn();\n      allow update: if isAdmin();\n      \n      // STATUS HISTORY SUBCOLLECTION\n      match /statusHistory/{historyId} {\n         allow read: if isAdmin() || get(/databases/$(database)/documents/applications/$(applicationId)).data.userId == request.auth.uid;\n         allow create, update: if isAdmin();\n      }\n    }\n    \n    match /appointments/{appointmentId} {\n        allow list: if isAdmin();\n        allow read: if isAdmin() || resource.data.userId == request.auth.uid;\n        allow create: if isOwner(resource.data.userId);\n        allow update: if isAdmin();\n    }\n\n    // PUBLIC/AUTHENTICATED-READ COLLECTIONS\n    match /news/{newsId} {\n      allow read: if isSignedIn();\n      allow write: if isAdmin();\n    }\n    \n    match /forumPosts/{postId} {\n      allow read: if isSignedIn();\n      allow create: if isSignedIn();\n      allow delete: if isAdmin();\n    }\n\n    match /learningHub/{docId} {\n      allow read: if isSignedIn();\n      allow write: if isAdmin();\n    }\n    \n    match /settings/general {\n        allow read: if true;\n        allow write: if isAdmin();\n    }\n  }\n}\n"}]}}
